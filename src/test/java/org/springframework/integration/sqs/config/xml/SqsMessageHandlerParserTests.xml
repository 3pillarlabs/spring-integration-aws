<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:int="http://www.springframework.org/schema/integration"
  xmlns:int-sqs="http://www.springframework.org/schema/integration/sqs"
  xsi:schemaLocation="http://www.springframework.org/schema/integration http://www.springframework.org/schema/integration/spring-integration-2.1.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/integration/sqs http://www.springframework.org/schema/integration/sqs/spring-integration-sqs-2.2.xsd">

  <bean id="testQueue" class="com.threepillar.labs.si.test.support.BlockingQueueFactory" 
      factory-method="createBlockingQueue">
  </bean>

  <int:channel id="target"/>

  <int-sqs:outbound-channel-adapter id="sqsOutboundChannelAdapter" 
      queue-name="testQueue" 
      queue="testQueue"
      channel="target">
  </int-sqs:outbound-channel-adapter>

</beans>
